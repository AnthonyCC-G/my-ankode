# ============================================
# SYMFONY - CREDENTIALS BIDONS (VERSIONNÉ)
# ============================================
# Vrais credentials dans backend/.env.local (non versionné)

###> symfony/framework-bundle ###
APP_ENV=dev
APP_SECRET=change_this_secret_key_in_production_1234567890abcdef
APP_SHARE_DIR=var/share
###< symfony/framework-bundle ###

###> symfony/routing ###
DEFAULT_URI=http://localhost
###< symfony/routing ###

###> doctrine/doctrine-bundle ###
# DOCKER : Utilise les noms de services Docker
DATABASE_URL="postgresql://demo_user:demo_password@postgres:5432/demo_db?serverVersion=16&charset=utf8"
###< doctrine/doctrine-bundle ###

###> mongodb/doctrine-odm-bundle ###
# DOCKER : Utilise le nom de service Docker
MONGODB_URL=mongodb://demo_mongo:demo_mongo_pass@mongo:27017
MONGODB_DB=demo_db
###< mongodb/doctrine-odm-bundle ###

###> symfony/messenger ###
MESSENGER_TRANSPORT_DSN=doctrine://default?auto_setup=0
###< symfony/messenger ###

###> symfony/mailer ###
MAILER_DSN=null://null
###< symfony/mailer ###